{
  "name": "functions",
  "version": "1.0.0",
  "scripts": {
    "production": "npm run cred:prod && firebase deploy --only functions:gcp_exportables_pdf:pdf-keralty",
    "lint": "eslint --ext .js,.ts .",
    "logs": "firebase functions:log",
    "build": "tsc",
    "watch": "tsc --watch",
    "cred:dev": "infisical export --env=dev --path=/keralty/infrastructure/exportables > .env",
    "cred:staging": "infisical export --env=staging --path=/keralty/infrastructure/exportables > .env",
    "cred:prod": "infisical export --env=prod --path=/keralty/infrastructure/exportables > .env",
    "gcp-build": "node node_modules/puppeteer/install.mjs"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.2",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.1",
    "handlebars": "^4.7.7",
    "puppeteer": "^22.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "private": true
}
